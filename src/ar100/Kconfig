# Kconfig settings for AR100

if MACH_AR100

config AR100_SELECT
    bool
    default y
    select LOW_LEVEL_OPTIONS
    select HAVE_ENDSTOPS
    select HAVE_STEPPERS
    select HAVE_GPIO

config BOARD_DIRECTORY
    string
    default "ar100"

config CLOCK_FREQ
    int
    default 300000000

config STEP_DELAY
    default 0

endif
